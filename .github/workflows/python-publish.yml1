name: Python package

on: [push]

jobs:
  build:

    runs-on: ubuntu-latest
    strategy:
      matrix:
        Nums : [516, 234, 789, 342, 133]

    steps:
      - uses: actions/checkout@v3
      - name: Set up Python ${{ 3.9 }}
        uses: actions/setup-python@v4
        with:
          python-version: ${{ 3.9 }}
      - name: Install dependencies
        run: |
          python -m pip install --upgrade pip
      - name: Test
        run: |
          ex-3.py
          ${{ Nums }}
